{
  "packageManager": "yarn@3.6.3",
  "name": "ng16-jest-preset",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test:karma:headless": "ng test --browsers ChromeHeadless --watch false",
    "test": "ng test",
    "test:jest": "yarn jest-esm --no-cache",
    "jest-esm": "yarn node --experimental-vm-modules --experimental-worker --no-warnings $(yarn bin jest) --maxWorkers=6"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/common": "^16.2.7",
    "@angular/compiler": "^16.2.7",
    "@angular/core": "^16.2.7",
    "@angular/forms": "^16.2.7",
    "@angular/platform-browser": "^16.2.7",
    "@angular/platform-browser-dynamic": "^16.2.7",
    "@angular/router": "^16.2.7",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.7",
    "@types/jasmine": "~4.3.6",
    "@types/jest": "^29.5.5",
    "jasmine-core": "~5.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "12.2.6",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^16.2.3",
    "ts-node": "^10.9.1",
    "typescript": "<5.0"
  }
}
